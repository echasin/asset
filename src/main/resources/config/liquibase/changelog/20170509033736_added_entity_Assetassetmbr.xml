<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd
                        http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">

    <property name="now" value="now()" dbms="h2"/>

    <property name="now" value="current_timestamp" dbms="postgresql"/>

    <property name="floatType" value="float4" dbms="postgresql, h2"/>
    <property name="floatType" value="float" dbms="mysql, oracle, mssql"/>

    <!--
        Added the entity Assetassetmbr.
    -->
    <changeSet id="20170509033736-1" author="jhipster">
        <createTable tableName="assetassetmbr">
            <column name="id" type="bigint" autoIncrement="${autoIncrement}">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="comment" type="varchar(100)">
                <constraints nullable="true" />
            </column>

            <column name="parentxcoordinate" type="integer">
                <constraints nullable="true" />
            </column>

            <column name="parentycoordinate" type="integer">
                <constraints nullable="true" />
            </column>
            
            <column name="childxcoordinate" type="integer">
                <constraints nullable="true" />
            </column>

            <column name="childycoordinate" type="integer">
                <constraints nullable="true" />
            </column>

            <column name="parentinstance" type="varchar(100)">
                <constraints nullable="true" />
            </column>

            <column name="childinstance" type="varchar(100)">
                <constraints nullable="true" />
            </column>

            <column name="description" type="varchar(255)">
                <constraints nullable="true" />
            </column>

            <column name="status" type="varchar(25)">
                <constraints nullable="false" />
            </column>

            <column name="lastmodifiedby" type="varchar(50)">
                <constraints nullable="false" />
            </column>

            <column name="lastmodifieddatetime" type="timestamp">
                <constraints nullable="true" />
            </column>

            <column name="domain" type="varchar(25)">
                <constraints nullable="true" />
            </column>

            <column name="parentasset_id" type="bigint">
                <constraints nullable="false" />
            </column>

            <column name="childasset_id" type="bigint">
                <constraints nullable="false" />
            </column>

            <column name="model_id" type="bigint">
                <constraints nullable="false" />
            </column>

            <!-- jhipster-needle-liquibase-add-column - JHipster will add columns here, do not remove-->
        </createTable>
        <dropDefaultValue tableName="assetassetmbr" columnName="lastmodifieddatetime" columnDataType="datetime"/>
        
    </changeSet>
</databaseChangeLog>
